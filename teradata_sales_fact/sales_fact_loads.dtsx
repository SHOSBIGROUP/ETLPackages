              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	mst.TRS_DT,
	mst.LOC_NO,
	sum(coalesce(spp.NetSales,0)) as spp_sales,
	0 as spp_units,
	sum(coalesce(spm.NetSales,0)) as spp_merch_sales,
	sum(coalesce(blk.bulk_sales,0)) as bulk_sales,
	sum(coalesce(blk.bulk_unit,0)) as bulk_units,
	sum(coalesce(crd.SCARDAM,0)) as sears_credit_sales,
	sum(coalesce(crd.TOT_MINUS_GC,0)) as other_credit_sales

FROM 
	(	SELECT TRS_DT, LOC_NO
		FROM teradata.td_sales_dly_sales
		GROUP BY TRS_DT, LOC_NO
	) mst
	
	LEFT JOIN teradata.td_sales_dly_spp spp ON (mst.LOC_NO = spp.LOC_NO AND mst.TRS_DT = spp.TRS_DT)
	LEFT JOIN teradata.td_sales_dly_spp_merch spm ON (mst.LOC_NO = spm.LOC_NO AND mst.TRS_DT = spm.TRS_DT)
	LEFT JOIN teradata.td_sales_dly_bulk_sales blk ON (mst.LOC_NO = blk.LOC_NO AND mst.TRS_DT = blk.TRS_DT)
	LEFT JOIN teradata.td_sales_dly_credit_share crd ON (mst.LOC_NO = crd.LOC_NO AND mst.TRS_DT = crd.TRS_DT)

GROUP BY mst.trs_dt, mst.LOC_NO</property>
