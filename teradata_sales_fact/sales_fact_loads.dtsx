                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--Join in all raw tables to feed into ssis to load fact
SELECT
	net.TRS_DT,
	net.LOC_NO,
	sum(net.netsales) as net_sales,
	sum(net.netunits) as net_units,
	sum(coalesce(pa.NetSales,0)) as pa_sales,
	sum(coalesce(pa.pamunits,0)) as pa_units,
	sum(coalesce(pam.NetSales,0)) as pam_sales,
	sum(coalesce(pam.pamunits,0)) as pam_units,
	sum(coalesce(mkd.PRO_MDS,0) + coalesce(mkd.REG_MDS,0)) as total_markdowns,
	sum(coalesce(avt.NETSLS,0)) as avg_ticket_amt,
	sum(coalesce(avt.UNITQTY,0)) as avg_ticket_units,
	sum(coalesce(lse.LSESALES,0)) as lease_sales,
	sum(coalesce(lse.LSEUNITS,0)) as lease_units

FROM teradata.td_sales_dly_sales net
	LEFT JOIN teradata.td_sales_dly_pa pa ON (net.DIV_NO = pa.Div_NO AND net.LOC_NO = pa.LOC_NO AND net.TRS_DT = pa.TRS_DT)
	LEFT JOIN teradata.td_sales_dly_pa_merch pam ON (net.DIV_NO = pam.POS_DIV AND net.LOC_NO = pam.LOC_NO AND net.TRS_DT = pam.TRS_DT)
	LEFT JOIN teradata.td_sales_dly_markdowns mkd ON (net.DIV_NO = mkd.DIV_NO AND net.LOC_NO = mkd.LOC_NO AND net.TRS_DT = mkd.MKDN_DT)
	LEFT JOIN teradata.td_sales_dly_avg_ticket avt ON (net.DIV_NO = avt.DIV_NO AND net.LOC_NO = avt.LOC_NO AND net.TRS_DT = avt.TRS_DT)
