                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select   a.[Transaction Document Number] ,[sho type],[transaction status], cast(trim([paid date]) as date) as paid_date ,
cast(trim(store) as varchar) as store,trim(location) as location,RIGHT('000'+CAST(a.division AS VARCHAR(3)),3) as division,
RIGHT('000'+CAST(line AS VARCHAR(3)),3) as line,cast(trim(category) as varchar) as category,  
case when  a.[item name] is not null then RIGHT('000000000'+CAST(a.[item name] AS VARCHAR(9)),6) else '-1' end  as  item_name,
 --cast(trim(a.[Net Amount]) as decimal(12,2))  
 a.[Net Amount]  as net_amount ,cast(trim([Qty]) as int) as qty,
case when upper([brand]) like '%KENMORE%' then a.[Net amount] end as brand_sales,
case when upper([brand]) like '%KENMORE%' then [qty] end as brand_units,
case when upper([brand]) like '%CRAFTSMAN%' then a.[Net amount] end as craftsman_sales,
case when upper([brand]) like '%CRAFTSMAN%' then [qty] end as craftsman_units,
case when upper([brand]) like '%DIEHARD%' then a.[Net amount] end as diehard_sales,
case when upper([brand]) like '%DIEHARD%' then [qty] end as diehard_units,
 case when  ( a.[division] = 22 and  cast([LINE] as int) in (  22,21,15,9,25,20,41) ) or   
       ( a.[division] = 26 and   cast([LINE] as int)   in ( 1,2,4))  or
       ( a.[division] = 46 and   cast([LINE] as int)   in ( 1,2,4,51))
     then a.[Net amount] else '0' end as avg_ticket_sales,
  case when  ( a.[division] = 22 and  cast([LINE] as int) in (  22,21,15,9,25,20,41) ) or   
       ( a.[division] = 26 and   cast([LINE] as int)   in ( 1,2,4))  or
       ( a.[division] = 46 and   cast([LINE] as int)   in ( 1,2,4,51))
     then [qty] else '0' end as avg_ticket_units,
   b.[Net amount]  as lease_sales,
    case when b.[Transaction Document Number] is not null then 1 else 0 end as lease_units,
case when upper([Available PA]) like '%PA%' then a.[net amount] else '0' end PAm_Sales,
  case when upper([Available PA]) like '%PA%' then [qty] else '0' end PAm_units,
   case when upper([Available PA]) like '%SP%' then a.[net amount] else '0' end spp_merch_Sales,
  case when upper([Available PA]) like '%SP%' then [qty] else '0' end spp_merch_units,
  case when upper([IS PA]) like 'TTT' then a.[net amount] else '0' end PA_Sales,
  case when upper([IS PA]) like 'TTT' then [qty] else '0' end PA_units,
  case when upper(a.[item name]) like '%SP%' then a.[net amount] else '0' end spp_sales,
  case when upper(a.[item name]) like '%SP%' then [qty] else '0' end spp_units,
 '0'   as sears_sales,  0 as sears_units,   '0'   as other_card_sales,
0 as other_card_units,
case when upper([Bulk Sale Tran]) like 'T' then a.[Net amount] else '0' end  as bulk_sales,
case when upper([Bulk Sale Tran]) like 'T' then [qty] else '0' end  as bulk_units,
  cast(trim([Gross Profit]) as decimal(12,2))  as Gross_Profit ,
  case when  ( a.[division] = 25 and   RIGHT('000000'+CAST(isnull(a.[item name],0) AS VARCHAR(6)),6) in (12430, 12431, 12432, 12434, 12435, 12436, 99111,99112, 99113, 99114, 99115, 99116, 99117, 99118, 99119, 99110))
  then a.[Net Amount] end guardsman_sales,
   case when  ( a.[division] = 25 and   RIGHT('000000'+CAST(isnull(a.[item name],0) AS VARCHAR(6)),6) in (12430, 12431, 12432, 12434, 12435, 12436, 99111,99112, 99113, 99114, 99115, 99116, 99117, 99118, 99119, 99110))
  then [Qty] end guardsman_units,
  case when  cast(trim(a.[Net Amount]) as decimal(12,2)) &gt;=0 then cast(trim([Amount]) as decimal(12,2))-cast(trim(a.[Net Amount]) as decimal(12,2)) else 0 end as markdown_sales,
'N' Transaction_Source
from [dbo].[NS_Daily File_Sales_Orders] as a 
left outer  join
(
 select  [Transaction Document Number], Division,[Item Name],[Net Amount] from [dbo].[NS_Daily File_Sales_Orders] where   [division] &lt;&gt; ' ' and  [Division] &lt;&gt;70   and  [Transaction Document Number] 
  in --('INV4826','INV4825')
  (
 SELECT distinct [Transaction Document Number]   FROM [dbo].[NS_Daily_File_Sales_Orders_Header] where [Payment or Refund Method]   like '%Lease%'   )) b 
 on a.[Transaction Document Number]=b.[Transaction Document Number] and a.Division=b.Division and a.[Item Name]=b.[Item Name]
where a.[SHO Type] &lt;&gt;' '    and a.category &lt;&gt; ' '    and isnumeric(a.[Item Name]) =1  
 and (TRIM(upper(a.[Transaction Status])) not like  '%open%'   and TRIM(upper(a.[Transaction Status])) not like  '%NOT%DEPOSITED' )
 and [paid date] &lt;&gt; 'NEW ITEM'</property>
