        <SQLTask:SqlTaskData
          SQLTask:Connection="{45a5110b-53ae-4bb5-85f0-14588469e03e}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="--*** Update Category Division Surrogate Key in sho_staging.netsuite.product ***&#xA; Update [sho_staging].[netsuite].[product] set &#xA;  cat_div_sk_id = s.cat_div_sk_id from &#xA;  [sho_dw].[dim].[category_division] s&#xA;  inner join [sho_staging].[netsuite].[product] t &#xA;  on Convert(char(3),Right('000' + Trim(s.[div_nbr]),3)) = Convert(char(3),Right('000' + Trim(t.[Div No]),3)) and &#xA;  Convert(char(3),Right('000' + Trim(s.[cat_nbr]),3)) = Convert(char(3),Right('000' + Trim(t.[Category No]),3));&#xA;&#xA;--*** Update Existing Products from Netsuite in sho_staging.netsuite.product in sho_dw.dim.product ***&#xA;Update sho_dw.dim.product Set &#xA;[cat_div_sk_id] = Convert(int,s.[cat_div_sk_id]),&#xA;[inactive] = CASE WHEN (UPPER(s.Inactive) Like 'Y%') THEN 1 Else 0 END,&#xA;[cat_nbr] = Convert(char(3),Right('000' + Trim([Category No]),3)),&#xA;[cat_name] = Convert(varchar(100),Trim([Category Name])),&#xA;[div_nbr] = Convert(char(3),Right('000' + Trim([Div No]),3)),&#xA;[div_name] = Convert(varchar(100),Trim([Div Name])),&#xA;[ln_nbr] = Convert(char(3),Right('000' + Trim([Line No]),3)),&#xA;[ln_desc] = Convert(varchar(200),Trim([Line Name])),&#xA;[sbl_nbr] = Convert(char(3),Right('000' + Trim([Subline No]),3)),&#xA;[sbl_desc] = Convert(varchar(200),Trim([Subline Name])),&#xA;[cls_nbr] = Convert(char(3),Right('000' + Trim([Class No]),3)),&#xA;[cls_desc] = Convert(varchar(200),Trim([Class Name])),&#xA;[item_nbr] = Substring(Trim([item no]),4,6),&#xA;[item_desc] = Convert(varchar(200),Trim([Description])),&#xA;[sku_nbr] = Convert(char(3),Right('000' + Trim([SKU #]),3)),&#xA;[upc_code] = Convert(varchar(50),Trim([UPC Code])),&#xA;[manufacturer_nbr] = Convert(varchar(50),Trim([Item Mfgn Number])),&#xA;[manufacturer_name] = Convert(varchar(100),Trim([Manufacturer])),&#xA;[brand_id] = Convert(varchar(50),Trim([Product Brand ID])),&#xA;[brand_desc] = Convert(varchar(500),Trim([Item Brand])),&#xA;[contract_item_type] = Convert(varchar(5),Trim([Contract Item Type])),&#xA;[height] = Convert(float,Trim([Height (in.)])),&#xA;[width] = Convert(float,Trim([Width (in.)])),&#xA;--[shipping_height] = Convert(decimal,Trim([Shipping Height])),&#xA;--[shipping_width] = Convert(decimal,Trim([Shipping Width])),&#xA;--[list_price] = Convert(decimal,Trim([List Price (Outlet)])),&#xA;--[regular_price] = Convert(decimal,Trim([Regular Price (3H)])),&#xA;--[msrp_price] = Convert(decimal,Trim([MSRP])),&#xA;--[weighted_avg_cost] = Convert(decimal,Trim([Average Cost])),&#xA;[rec_upd_dt] = GETDATE()&#xA; From [sho_staging].[netsuite].[product] s&#xA; Inner Join sho_dw.dim.product t On &#xA;Convert(varchar(50),Trim(s.[Item No])) = (Trim(t.[div_nbr]) + trim(t.[item_nbr]) + trim(t.[sku_nbr])) &#xA;--where &#xA;--ISNUMERIC(s.[shipping height]) &lt;&gt; 0 &#xA;--and ISNUMERIC(s.[shipping width]) &lt;&gt; 0&#xA;--and ISNUMERIC(s.[List Price (Outlet)]) &lt;&gt; 0 &#xA;--and ISNUMERIC(s.[Regular Price (3H)]) &lt;&gt; 0 &#xA;--and ISNUMERIC(s.[MSRP]) &lt;&gt; 0 &#xA;--and ISNUMERIC(s.[Average Cost]) &lt;&gt; 0;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
