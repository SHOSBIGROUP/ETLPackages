        <SQLTask:SqlTaskData
          SQLTask:Connection="{45a5110b-53ae-4bb5-85f0-14588469e03e}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="--*** Insert New Products from Netsuite in sho_staging.netsuite.product in sho_dw.dim.product ***&#xA;Insert Into sho_dw.dim.product(&#xA;[cat_div_sk_id],&#xA;[inactive],&#xA;[cat_nbr],&#xA;[cat_name],&#xA;[div_nbr],&#xA;[div_name],&#xA;[ln_nbr],&#xA;[ln_desc],&#xA;[sbl_nbr],&#xA;[sbl_desc],&#xA;[cls_nbr],&#xA;[cls_desc],&#xA;[item_nbr],&#xA;[item_desc],&#xA;[sku_nbr],&#xA;[upc_code],&#xA;[manufacturer_nbr],&#xA;[manufacturer_name],&#xA;[brand_id],&#xA;[brand_desc],&#xA;[contract_item_type],&#xA;[height],&#xA;[width],&#xA;[shipping_height],&#xA;[shipping_width],&#xA;[list_price],&#xA;[regular_price],&#xA;[msrp_price],&#xA;[weighted_avg_cost],&#xA;[rec_load_dt],&#xA;[rec_upd_dt]&#xA;) Select &#xA;Convert(int,[cat_div_sk_id]),&#xA;CASE WHEN (UPPER(s.Inactive) Like 'Y%') THEN 1 Else 0 END,&#xA;Convert(char(3),Right('000' + Trim([Category No]),3)),&#xA;Convert(varchar(100),Trim([Category Name])),&#xA;Convert(char(3),Right('000' + Trim([Div No]),3)),&#xA;Convert(varchar(100),Trim([Div Name])),&#xA;Convert(char(3),Right('000' + Trim([Line No]),3)),&#xA;Convert(varchar(200),Trim([Line Name])),&#xA;Convert(char(3),Right('000' + Trim([Subline No]),3)),&#xA;Convert(varchar(200),Trim([Subline Name])),&#xA;Convert(char(3),Right('000' + Trim([Class No]),3)),&#xA;Convert(varchar(200),Trim([Class Name])),&#xA;Substring(Trim([item no]),4,6),&#xA;Convert(varchar(200),Trim([Description])),&#xA;Convert(char(3),Right('000' + Trim([SKU #]),3)),&#xA;Convert(varchar(50),Trim([UPC Code])),&#xA;Convert(varchar(50),Trim([Item Mfgn Number])),&#xA;Convert(varchar(100),Trim([Manufacturer])),&#xA;Convert(varchar(50),Trim([Product Brand ID])),&#xA;Convert(varchar(500),Trim([Item Brand])),&#xA;Convert(varchar(5),Trim([Contract Item Type])),&#xA;Convert(float,Trim([Height (in.)])),&#xA;Convert(float,Trim([Width (in.)])),&#xA;Convert(decimal,Trim([Shipping Height])),&#xA;Convert(decimal,Trim([Shipping Width])),&#xA;Convert(decimal,Trim([List Price (Outlet)])),&#xA;Convert(decimal,Trim([Regular Price (3H)])),&#xA;Convert(decimal,Trim([MSRP])),&#xA;Convert(decimal,Trim([Average Cost])),&#xA;GETDATE(),&#xA;GETDATE()&#xA; From [sho_staging].[netsuite].[product] s &#xA; where Convert(varchar(50),Trim(s.[Item No])) &#xA; Not In (select Trim([div_nbr]) + trim([item_nbr]) + trim([sku_nbr]) From sho_dw.dim.product);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
