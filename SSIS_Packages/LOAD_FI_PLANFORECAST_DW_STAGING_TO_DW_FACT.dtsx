          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="UPDATE sho_staging.finance.day_location_sales_plan set daily_sales = 0 where daily_sales like '%E%';&#xA;&#xA;UPDATE sho_staging.finance.day_location_gp_plan set gp_plan = 0 where gp_plan like '%E%';&#xA;&#xA;Insert Into sho_dw.fact.sho_sales_plan_gp_by_loc(&#xA;[sho_location_sk_id],&#xA;[plan_dt_sk_id],&#xA;[plan_dt],&#xA;[pos_nbr],&#xA;[sales_plan_amt],&#xA;[gp_plan_amt]&#xA;)&#xA;Select &#xA;-1,&#xA;convert(varchar(8),convert(int,Convert(varchar(10),convert(datetime,dlsp.day_of_month),112))),&#xA;Convert(date,dlsp.[day_of_month]),&#xA;Convert(int,dlsp.[pos_nbr]),&#xA;convert(decimal(11,2),REPLACE(REPLACE(REPLACE(REPLACE(TRIM(dlsp.daily_sales),'$',''),',',''),'(',''),')','')),&#xA;convert(decimal(11,2),REPLACE(REPLACE(REPLACE(REPLACE(TRIM(dlgp.gp_plan),'$',''),',',''),'(',''),')',''))&#xA; From sho_staging.finance.day_location_sales_plan dlsp &#xA; LEFT OUTER JOIN sho_staging.finance.day_location_gp_plan dlgp  &#xA; on convert(varchar(8),convert(int,Convert(varchar(10),convert(datetime,dlsp.day_of_month),112))) = convert(varchar(8),convert(int,Convert(varchar(10),convert(datetime,dlgp.day_of_month),112)))&#xA; AND convert(varchar(5),dlsp.pos_nbr) = convert(varchar(5),dlgp.pos_nbr) &#xA;where dlsp.daily_sales not like '%E%' &#xA;and dlgp.gp_plan not like '%E%'&#xA;and&#x9;convert(varchar(8),convert(int,Convert(varchar(10),convert(datetime,dlsp.day_of_month),112))) + '|' + convert(varchar(5),dlsp.pos_nbr) not in &#xA;&#x9;(select convert(varchar(8),plan_dt_sk_id) + '|' + convert(varchar(5),pos_nbr) from [sho_dw].[fact].[sho_sales_plan_gp_by_loc]);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
