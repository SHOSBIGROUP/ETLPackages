            <component
              refId="Package\Data Flow Task\Payment Source"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Payment Source"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select credit_sales.tran_date,credit_sales.store,credit_sales.Sears_Card_Credit_Sales,credit_sales.non_sears_card_Credit_sales,Coalesce(apo_sales.apo_apps,0) as apo_apps,Coalesce(apo_sales.apo_opps,0) as apo_opps  from (
select  [eis_number] as store,[tran_date], sum(Sears_Card_Credit_Sales) as Sears_Card_Credit_Sales,
sum(non_sears_card_Credit_sales) as non_sears_card_Credit_sales from (
select [eis_number],[tran_date],[transaction_type],
case when [transaction_type] = 'creditmemo' then sum(Sears_Card_Payment)*-1 else sum(Sears_Card_Payment) end  as Sears_Card_Credit_Sales,
case when [transaction_type] = 'creditmemo' then sum(Other_Card_Payment)*-1 else sum(Other_Card_Payment) end as non_sears_card_Credit_sales
--,sum(Lease_Payment) as Lease_Sales,sum(Lease_Count) as Lease_Sales_Count
from (
select [eis_number],[create_date] as tran_date,[Payment_Method],[transaction_type], -- cast([payment Amount] as float)  as ss
 case when (trim([Payment_Method]) like 'Sears Commercial%' or trim([Payment_Method]) like 'Sears MasterCard%' or  trim([Payment_Method]) like 'Sears Private%')   then sum(cast([amount] as float)) else 0 end as Sears_Card_Payment,
 case when (trim([Payment_Method]) not like 'Sears Commercial%' and trim([Payment_Method]) not like 'Sears MasterCard%' and   trim([Payment_Method]) not like 'Sears Private%' and   trim([Payment_Method]) not like 'Sears G%')  then sum(cast([amount] as float)) else 0 end  as Other_Card_Payment--,
from [netsuite].[sales_daily_payment_history] Where  
  eis_number  in (select eis_nbr from sho_dw.dim.location where IsConverted &lt;&gt; 0)
group by  [eis_number],[create_date] ,[transaction_type],[Payment_Method] ) as aa group by [eis_number],[tran_date],[transaction_type]
) as aa
group by [eis_number],[tran_date]) as credit_sales
left outer join
(

select opps.location,opps.trs_dt,opps.apo_opps,coalesce(apps.apo_apps,0) as apo_apps  from (

select 
SUBSTRING(apo.[date_created], 1, CHARINDEX(' ', apo.[date_created])-1)  as trs_dt, 
 trim(right(location,5)) as location,
Coalesce(count (distinct apo.[Sales_Check_nummber]),0) as apo_opps 
from sho_staging.netsuite.[sales_daily_apo_history] apo
inner join [sho_staging].[netsuite].[sales_daily_payment_history] header 
on TRIM(apo.[transaction_document_number]) = TRIM(header.[sales_document_number]) 
where len(trim(header.[sales_check_number])) &gt; 0 
and trim(header.[payment_method]) not like 'Sears MasterCard%' 
and trim(header.[Payment_Method]) not like 'Sears Private Label%' 
GROUP BY SUBSTRING(apo.[date_created], 1, CHARINDEX(' ', apo.[date_created])-1),location

) as opps


left outer join
(select SUBSTRING(apo.[date_created], 1, CHARINDEX(' ', apo.[date_created])-1) as trs_dt, trim(right(location,5)) as location, Coalesce(count(*),0) as apo_apps  from sho_staging.netsuite.[sales_daily_apo_history] apo
inner join [sho_staging].[netsuite].[sales_daily_payment_history] header 
on TRIM(apo.[transaction_document_number]) = TRIM(header.[sales_document_number]) 
where apo.SHO_REASON_CODE = 'SCAP' 
GROUP BY SUBSTRING(apo.[date_created], 1, CHARINDEX(' ', apo.[date_created])-1), location

) 


as apps
on apps.location=opps.location and apps.trs_dt=opps.trs_dt) apo_sales
on credit_sales.store=apo_sales.location and credit_sales.tran_date=apo_sales.trs_dt
;</property>
