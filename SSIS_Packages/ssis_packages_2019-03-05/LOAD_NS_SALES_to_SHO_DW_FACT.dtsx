                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--- valid query


select 
a.[Document_Number],
a.[SHO_ITEM_TYPE], a.[transaction_status],-- a.[transaction_type],a.[created_from_type],
  a.[create_date]   as paid_date ,
 cast(trim(a.store) as varchar(4)) as store,
 RIGHT('000' + trim(LEFT(a.division,3)),3) as division,
 RIGHT('00' + trim(LEFT(a.line,2)),2) as line,
 RIGHT('000' + trim(LEFT(a.category,3)),3) as category, 
 RIGHT('000' + LEFT(a.sku_number,3),3) as sku, 
case when trim(a.item_name)='Open Item (Bin)'  then a.item_name else substring(a.item_name,4,6)   end as item_name,
  a.[net_amt]   as net_amt_decimal,
 a.[net_amt]  as net_amount ,  a.[net_qty]  as qty,
case when upper(a.[brand]) like '%KENMORE%' then a.[net_amt] end as brand_sales,
case when upper(a.[brand]) like '%KENMORE%' then a.[net_qty] end as brand_units,
case when upper(a.[brand]) like '%CRAFTSMAN%' then a.[net_amt] end as craftsman_sales,
case when upper(a.[brand]) like '%CRAFTSMAN%' then a.[net_qty] end as craftsman_units,
case when upper(a.[brand]) like '%DIEHARD%' then a.[net_amt] end as diehard_sales,
case when upper(a.[brand]) like '%DIEHARD%' then a.[net_qty] end as diehard_units,
 case when  ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2) = 22 and  cast(a.[LINE] as int) in (  22,21,15,9,25,20,41) ) or   
       ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2) =26 and   cast(a.[LINE] as int)   in ( 1,2,4))  or
       ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2) = 46 and   cast(a.[LINE] as int)   in ( 1,2,4,51))
     then a.[net_amt] else '0' end as avg_ticket_sales,
  case when  ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2) = 22 and  cast(a.[LINE] as int) in (  22,21,15,9,25,20,41) ) or   
       ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2) = 26 and   cast(a.[LINE] as int)   in ( 1,2,4))  or
       ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2)= 46 and   cast(a.[LINE] as int)   in ( 1,2,4,51))
     then a.[net_qty] else '0' end as avg_ticket_units,
    b.[net_amt]  as lease_sales,
    case when b.[Document_Number] is not null then 1 else 0 end as lease_units,
	case when upper(a.[available_pa]) like '%PA%' AND upper(a.[Bulk_Sales]) like 'F' AND a.[create_date] &gt;= '2019-02-16' then a.[net_amt] else '0' end PAm_Sales,
	case when upper(a.[available_pa]) like '%PA%' AND upper(a.[Bulk_Sales]) like 'F' AND a.[create_date] &gt;= '2019-02-16' then a.[net_qty] else '0' end PAm_units,
   case when upper(a.[available_pa]) like '%SP%' then a.[net_amt] else '0' end spp_merch_Sales,
  case when upper(a.[available_pa]) like '%SP%' then a.[net_qty] else '0' end spp_merch_units,
  case when upper(a.[sho_item_type]) like '%PROTECTION AGREEMENT%' then a.[net_amt] else '0' end PA_Sales,
  case when upper(a.[SHO_ITEM_TYPE]) like '%PROTECTION AGREEMENT%'  then a.[net_qty] else '0' end PA_units,
  case when upper(a.[SHO_ITEM_TYPE]) like '%SEARS PROTECTION PLAN%' then a.[net_amt] else '0' end spp_sales,
  case when upper(a.[SHO_ITEM_TYPE]) like '%SEARS PROTECTION PLAN%' then a.[net_qty] else '0' end spp_units,
 '0'   as sears_sales,  0 as sears_units,   '0'   as other_card_sales,
0 as other_card_units,
 case when upper(a.[Bulk_Sales]) like 'T' then a.[net_amt] else '0' end  as bulk_sales,
 case when upper(a.[bulk_sales]) like 'T' then a.[net_qty] else '0' end  as bulk_units,
 a.[gross_profit]   as Gross_Profit ,
  case when  ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2) = 25 and   RIGHT('000000'+CAST(isnull(a.[item_name],0) AS VARCHAR(6)),6) in (12430, 12431, 12432, 12434, 12435, 12436, 99111,99112, 99113, 99114, 99115, 99116, 99117, 99118, 99119, 99110))
  then a.[net_amt] end guardsman_sales,
   case when  ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2) = 25 and   RIGHT('000000'+CAST(isnull(a.[item_name],0) AS VARCHAR(6)),6) in (12430, 12431, 12432, 12434, 12435, 12436, 99111,99112, 99113, 99114, 99115, 99116, 99117, 99118, 99119, 99110))
  then a.[net_qty] end guardsman_units,
 case when  a.[net_amt]   &gt;=0 then  a.[sold_price] -  a.[net_amt]   else 0 end as markdown_sales,
'N' Transaction_Source,
case when upper(a.[Transaction_Source]) like '%E-Commerce%' then a.[net_amt] else '0' end  as online_sales,
case when upper(a.[Transaction_Source]) like '%E-Commerce%' then a.[net_qty] else '0' end  as online_units,
case when ((trim(a.[transaction_type])='Credit Memo' and trim(a.[transaction_status])='Fully Applied' ) or ( trim( a.[transaction_type])='Invoice' and trim(a.[transaction_status])='Paid In Full')) then a.[net_amt] else 0 end as revenue_sales 
  from 
 netsuite.sales_daily_line_history  a
 left outer join 
 (
 select [Document_Number], transaction_type, RIGHT('000' + trim(LEFT(division,3)),3) as division,substring(item_name,4,6) as item_name,
 [net_amt],[sku_number],[line_sequence_number]  from netsuite.sales_daily_line_history  
 where [division] &lt;&gt; ' ' and RIGHT('000' + trim(LEFT(division,3)),3) &lt;&gt; 70 and [Document_Number] 
  in 
  (
   select distinct max([sales_document_number]) from [netsuite].[sales_daily_payment_history] where internal_id in 
   (
     select internal_id from [netsuite].[sales_daily_payment_history] where [payment_method] like '%Lease%'
   ) 
   group by internal_id  
   UNION ALL 
   select distinct max([credit_memo]) from [netsuite].[sales_daily_payment_history] where internal_id in 
   (
     select internal_id from [netsuite].[sales_daily_payment_history] where [payment_method] like '%Lease%'
   ) 
     group by internal_id  
  )
  ) b 
 on b.[document_number] =a.[Document_Number]   and RIGHT('000' + trim(LEFT(a.division,3)),3)=b.Division  and substring(a.item_name,4,6)=b.[Item_Name]  and a.[line_sequence_number] =b.[line_sequence_number] 
  where 
  a.[sho_item_type] &lt;&gt;' '    and
 a.category &lt;&gt; ' '    and 
(   isnumeric(substring( a.item_name,4,6)) =1 or  a.item_name like '%Open%Bin%'  )
  and a.store is not null
  and (
       not((trim(a.[transaction_type])='Credit Memo' and upper(trim(a.[transaction_status]))='VOIDED' ) )
   and not(( trim( a.[transaction_type])='Credit Memo' and upper(trim(a.[created_from_type])) like 'RETURN AUTH%' and a.[created_from_type] is not null))
   and not((trim(a.[transaction_type])='Invoice' and upper(trim(a.[transaction_status])) in ('REJECTED','PENDING APPROVAL')))
   and not(( trim( a.[transaction_type])='Invoice' and upper(trim(a.[created_from_type])) like 'SALES ORDER%' and a.[created_from_type] is not null))
   and not(( trim( a.[transaction_type])='Sales Order' and upper(trim(a.[transaction_status])) in ('PENDING APPROVAL','CANCELLED')))
   and not(( trim( a.[transaction_type])='Return Authorization' and upper(trim(a.[transaction_status])) in ('PENDING APPROVAL','CANCELLED','CLOSED'))))</property>
