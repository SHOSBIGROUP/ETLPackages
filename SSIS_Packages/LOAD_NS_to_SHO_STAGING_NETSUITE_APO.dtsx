      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{70e1c0af-32b1-4003-bbb8-6f11bcf67768}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="&#xA;--*** Update sho_dw.fact.sho_sales_by_loc with APO Opportunities data ***&#xA;Update sho_dw.fact.sho_sales_by_loc set apo_opps_ty = apo_ns.apo_opps_ty&#xA;FROM&#xA;(&#xA;select Convert(date,apo.[Date Created]) as trs_dt, location.pos_nbr, count (distinct apo.[Sales Check nummber]) as apo_opps_ty from sho_staging.netsuite.[NS_Daily_File_Sales_Orders_Header_APO] apo&#xA;inner join [sho_staging].[netsuite].[NS_Daily_File_Sales_Orders_Header] header &#xA;on TRIM(apo.[Transaction Document Number]) = TRIM(header.[Transaction Document Number]) &#xA;inner join [sho_dw].[dim].[location] location on &#xA;convert(int,header.Location) = location.eis_nbr&#xA;where len(trim(header.[sales check nummber])) &gt; 0 &#xA;GROUP BY Convert(date,apo.[Date Created]), location.pos_nbr&#xA;)&#xA;apo_ns&#xA;INNER JOIN &#xA;sho_dw.fact.sho_sales_by_loc fact ON &#xA;apo_ns.pos_nbr = fact.pos_nbr and apo_ns.trs_dt = fact.trs_dt;&#xA;&#xA;--*** Update sho_dw.fact.sho_sales_by_loc with APO Applications data ***&#xA;Update sho_dw.fact.sho_sales_by_loc set apo_apps_ty = apo_ns.apo_apps_ty&#xA;FROM&#xA;(&#xA;select Convert(date,apo.[Date Created]) as trs_dt, location.pos_nbr, count(*) as apo_apps_ty from sho_staging.netsuite.[NS_Daily_File_Sales_Orders_Header_APO] apo&#xA;inner join [sho_staging].[netsuite].[NS_Daily_File_Sales_Orders_Header] header &#xA;on TRIM(apo.[Transaction Document Number]) = TRIM(header.[Transaction Document Number]) &#xA;inner join [sho_dw].[dim].[location] location on &#xA;convert(int,header.Location) = location.eis_nbr&#xA;where apo.SHO_REASON_CODE = 'SCAP' &#xA;GROUP BY Convert(date,apo.[Date Created]), location.pos_nbr&#xA;)&#xA;apo_ns&#xA;INNER JOIN &#xA;sho_dw.fact.sho_sales_by_loc fact ON &#xA;apo_ns.pos_nbr = fact.pos_nbr and apo_ns.trs_dt = fact.trs_dt;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
