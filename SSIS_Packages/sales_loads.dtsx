              componentClassID="Attunity.SSISODBCSrc"
              contactInfo="ODBC Source;Connector for Open Database Connectivity (ODBC) by Attunity; Attunity Ltd.; All Rights Reserved; http://www.attunity.com;7"
              description="ODBC Source"
              name="Teradata PA Sales"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.String"
                  description="The name of the table to be fetched."
                  expressionType="Notify"
                  name="TableName"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand">-- PA  sales by div
SELECT 	S.TRS_DT,
		S.LOC_NO,
		P.LN_NO as DIV_NO,
		SUM((CASE WHEN S.TRS_TYP_CD='S' THEN 1 ELSE -1 END)*S.SLL_DLR) AS NetSales ,
		SUM((CASE WHEN S.TRS_TYP_CD='S' THEN 1 ELSE -1 END)*S.UN_QT) AS PAMUNITS 

FROM    
		SPRS_DW_VIEWS.SALES S 
		INNER JOIN shc_work_tbls.TS_SHO_locations L ON (S.LOC_NO=L.LOC_NO)
		INNER JOIN SPRS_DW_VIEWS.PRODUCT P ON (S.PRD_IRL_NO=P.PRD_IRL_NO)

WHERE   S.SLS_TYP_CD='M' 
	AND S.TRS_DT between date '2018-07-08' and date '2018-08-02'
	AND POS_DIV=70
	
GROUP BY 1,2,3</property>
