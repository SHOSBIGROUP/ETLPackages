                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- Get Max Date last generated
declare @max_date date = (select max(file_creation_date) from citi.file_generation_log where citi_file_type = 'line file');


SELECT 
	cast([create_date] as date) as 'TranDate'
,	[document_number] as 'Document Number'
,	right([accounting_unit],4) as 'Accounting Unit '
,	trim([store_format_type]) as 'Acc Unit Format'
,	[item_name] as 'Item Name'
,	[sku_number] as 'SKU #'
,	[sho_item_type] as 'SHO Item Type'
,	[sold_price] as 'Amount'
,	[net_amt] as 'Net Amount'
,	[net_qty] as 'Quantity'
,	[fulfillment_method] as 'Fulfillment Method'
,	[fulfillment_option_selected] as 'Fulfillment Option Selected'
,	[amount_tax] as 'Tax Amount'
,	[line_unique_key] as 'Line Unique Key'
,	[eis_location_num] as 'EIS Location Number'


FROM [sho_staging].[netsuite].[sales_daily_line_history]

WHERE
-- Line Specific Filtering (removed per avinash instructions: Removed so we are including special transactions such as overrides, discounts so that the tax totals balance)
	--[sho_item_type] &lt;&gt;' '    
	--and  category &lt;&gt; ' '    
	--and (   isnumeric(substring( item_name,4,6)) =1 or item_name like '%Open%Bin%'  )   

-- Header Specific Filtering
   not( (trim([transaction_type])='Credit Memo' and upper(trim([transaction_status]))='VOIDED' ) )
   and not( ( trim( [transaction_type])='Credit Memo' and upper(trim([created_from_type])) like 'RETURN AUTH%' and [created_from_type] is not null))
   and not( ( trim([transaction_type])='Invoice' and upper(trim([transaction_status])) in ('REJECTED','PENDING APPROVAL')))
   and not( ( trim( [transaction_type])='Invoice' and upper(trim([created_from_type])) like 'SALES ORDER%' and [created_from_type] is not null))
   and not( ( trim( [transaction_type])='Sales Order' and upper(trim([transaction_status])) in ('PENDING APPROVAL','CANCELLED')))
   and not( ( trim( [transaction_type])='Return Authorization' and upper(trim([transaction_status])) in ('PENDING APPROVAL','CANCELLED','CLOSED')))
   and store is not null
-- Apply Date Filter
   and rec_upd_dt  &gt; @max_date;</property>
