                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- Get Max Date last generated
declare @max_date date = (select max(file_creation_date) from citi.file_generation_log where citi_file_type = 'payment file');


SELECT 
	[payment_date] as 'Date'
,	[eis_nbr] as 'EIS Number'
,	[payment_type] as 'Transaction Type'
,	[payment_document_nbr] as 'Document Number'
,	[transaction_document_nbr] as 'Sales Document number'
,	[payment_method] as 'Payment Method'
,	[payment_amount] as 'Amount'
,	[creditcard_masked] as 'Masked Credit Card Number'
,	[internal_id] as 'internal id'

FROM sho_dw.fact.sho_sales_payment

-- Apply Date Filter
where rec_load_dt  &gt; @max_date;</property>
