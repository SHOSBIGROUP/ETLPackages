                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- Get Max Date last generated
declare @max_date date = (select max(file_creation_date) from citi.file_generation_log where citi_file_type = 'header file');

-- Source Query for Citi Header file
SELECT	cast([create_date] as datetime) as TranDate
,		[transaction_type] as 'Type'
,		[transaction_status] as 'Status'
,		[transaction_source] as 'Transaction Source'
,		[document_number] as 'Document Number'
,		[accounting_unit_nohierarchy] as 'Accounting Unit'
,		trim([store_format_type]) as 'Format'
,		[subtotal] as 'Subtotal'
,		[total_tax] as 'Tax Amount'
,		[shipping_cost] as 'Shipping Amount'
,		[total_amount] as 'Transaction Total Amount'
,		[sho_syw_memberid] as 'Shop Your Way Member id'
,		[amount_net_of_tax] as 'Total Amount without Tax'
,		cast([date_last_modified] as datetime) as 'Last Modified'
,		[amount_remaining] as 'Amount Remaining'


FROM [sho_staging].[netsuite].[sales_daily_header_history]
 
 WHERE
	-- Apply same filter criteria used in ns sales fact etl code
	  not( (trim([transaction_type])='Credit Memo' and upper(trim([transaction_status]))='VOIDED' ) )
   and not( ( trim( [transaction_type])='Credit Memo' and upper(trim([created_from_type])) like 'RETURN AUTH%' and [created_from_type] is not null))
   and not( ( trim([transaction_type])='Invoice' and upper(trim([transaction_status])) in ('REJECTED','PENDING APPROVAL')))
   and not( ( trim( [transaction_type])='Invoice' and upper(trim([created_from_type])) like 'SALES ORDER%' and [created_from_type] is not null))
   and not( ( trim( [transaction_type])='Sales Order' and upper(trim([transaction_status])) in ('PENDING APPROVAL','CANCELLED')))
   and not( ( trim( [transaction_type])='Return Authorization' and upper(trim([transaction_status])) in ('PENDING APPROVAL','CANCELLED','CLOSED')))
   
	-- Apply Date Filter
	and rec_upd_dt  &gt; @max_date;</property>
