                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- Get Max Date last generated
declare @max_date date = (select max(file_creation_date) from citi.file_generation_log where citi_file_type = 'comment file');


SELECT	 cast([apo].[date_created] as date) as 'Date'
,		 [hdr].[transaction_type] as 'Type'
,		 [apo].[transaction_document_number] as 'Document Number'
,		 [apo].[promotion] as 'Promotion'
,		 [apo].[applied_promotion_status] as 'Applied Promotion Status'
,        [apo].[sho_reason_code] as 'SHO Reason Code'
,		 [apo].[sho_pos_reason_code_comment] as 'SHO POS Reason Code Comment'
,		 [apo].[new_sears_card_application_status] as 'New Sears Card application Status'
,		 cast([hdr].[date_last_modified] as date) as 'Last Modified'
,		 [apo].[internal_id_header] as 'internal id'
,		 trim(char(13) from [apo].[internal_id_apo]) as 'comment internal id'

FROM [netsuite].[sales_daily_apo_history] apo
	INNER JOIN [netsuite].[sales_daily_header_history] hdr ON (apo.internal_id_header = hdr.internal_id)

WHERE
-- Header Specific Filtering
   not( (trim(hdr.[transaction_type])='Credit Memo' and upper(trim(hdr.[transaction_status]))='VOIDED' ) )
   and not( ( trim(hdr.[transaction_type])='Credit Memo' and upper(trim(hdr.[created_from_type])) like 'RETURN AUTH%' and hdr.[created_from_type] is not null))
   and not( ( trim(hdr.[transaction_type])='Invoice' and upper(trim(hdr.[transaction_status])) in ('REJECTED','PENDING APPROVAL')))
   and not( ( trim(hdr.[transaction_type])='Invoice' and upper(trim(hdr.[created_from_type])) like 'SALES ORDER%' and hdr.[created_from_type] is not null))
   and not( ( trim(hdr.[transaction_type])='Sales Order' and upper(trim(hdr.[transaction_status])) in ('PENDING APPROVAL','CANCELLED')))
   and not( ( trim(hdr.[transaction_type])='Return Authorization' and upper(trim(hdr.[transaction_status])) in ('PENDING APPROVAL','CANCELLED','CLOSED')))
   and hdr.store is not null

-- Apply Date Filter
	and apo.rec_upd_dt  &gt; @max_date;</property>
