                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
a.[Document_Number],
[SHO_ITEM_TYPE],--[transaction_status], 
 cast(trim([create_date]) as date) as paid_date ,
 cast(trim(store) as varchar(4)) as store,
 RIGHT('000' + trim(LEFT(a.division,3)),3) as division,
 RIGHT('00' + trim(LEFT(a.line,2)),2) as line,
 RIGHT('000' + trim(LEFT(a.category,3)),3) as category, 
 RIGHT('000' + LEFT(a.sku_number,3),3) as sku, 
substring(a.item_name,4,6) as item_name,
  a.[net_amt]   as net_amt_decimal,
 a.[net_amt]  as net_amount ,  [net_qty]  as qty,
case when upper([brand]) like '%KENMORE%' then a.[net_amt] end as brand_sales,
case when upper([brand]) like '%KENMORE%' then [net_qty] end as brand_units,
case when upper([brand]) like '%CRAFTSMAN%' then a.[net_amt] end as craftsman_sales,
case when upper([brand]) like '%CRAFTSMAN%' then [net_qty] end as craftsman_units,
case when upper([brand]) like '%DIEHARD%' then a.[net_amt] end as diehard_sales,
case when upper([brand]) like '%DIEHARD%' then [net_qty] end as diehard_units,
 case when  ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2) = 22 and  cast([LINE] as int) in (  22,21,15,9,25,20,41) ) or   
       ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2) =26 and   cast([LINE] as int)   in ( 1,2,4))  or
       ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2) = 46 and   cast([LINE] as int)   in ( 1,2,4,51))
     then a.[net_amt] else '0' end as avg_ticket_sales,
  case when  ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2) = 22 and  cast([LINE] as int) in (  22,21,15,9,25,20,41) ) or   
       ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2) = 26 and   cast([LINE] as int)   in ( 1,2,4))  or
       ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2)= 46 and   cast([LINE] as int)   in ( 1,2,4,51))
     then [net_qty] else '0' end as avg_ticket_units,
    b.[net_amt]  as lease_sales,
    case when b.[Document_Number] is not null then 1 else 0 end as lease_units,
case when upper([available_pa]) like '%PA%' then a.[net_amt] else '0' end PAm_Sales,
  case when upper([available_pa]) like '%PA%' then [net_qty] else '0' end PAm_units,
   case when upper([available_pa]) like '%SP%' then a.[net_amt] else '0' end spp_merch_Sales,
  case when upper([available_pa]) like '%SP%' then [net_qty] else '0' end spp_merch_units,
  case when upper([sho_item_type]) like '%PROTECTION AGREEMENT%' then a.[net_amt] else '0' end PA_Sales,
  case when upper([SHO_ITEM_TYPE]) like '%PROTECTION AGREEMENT%'  then [net_qty] else '0' end PA_units,
  case when upper(a.[SHO_ITEM_TYPE]) like '%SEARS PROTECTION PLAN%' then a.[net_amt] else '0' end spp_sales,
  case when upper(a.[SHO_ITEM_TYPE]) like '%SEARS PROTECTION PLAN%' then [net_qty] else '0' end spp_units,
 '0'   as sears_sales,  0 as sears_units,   '0'   as other_card_sales,
0 as other_card_units,
 case when upper([Bulk_Sales]) like 'T' then a.[net_amt] else '0' end  as bulk_sales,
 case when upper([bulk_sales]) like 'T' then [net_qty] else '0' end  as bulk_units,
 [gross_profit]   as Gross_Profit ,
  case when  ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2) = 25 and   RIGHT('000000'+CAST(isnull(a.[item_name],0) AS VARCHAR(6)),6) in (12430, 12431, 12432, 12434, 12435, 12436, 99111,99112, 99113, 99114, 99115, 99116, 99117, 99118, 99119, 99110))
  then a.[net_amt] end guardsman_sales,
   case when  ( LEFT('00'+CAST(a.division AS VARCHAR(2)),2) = 25 and   RIGHT('000000'+CAST(isnull(a.[item_name],0) AS VARCHAR(6)),6) in (12430, 12431, 12432, 12434, 12435, 12436, 99111,99112, 99113, 99114, 99115, 99116, 99117, 99118, 99119, 99110))
  then [net_qty] end guardsman_units,
 case when  a.[net_amt]   &gt;=0 then  [sold_price] -  a.[net_amt]   else 0 end as markdown_sales,
'N' Transaction_Source,
case when upper([Transaction_Source]) like '%E-Commerce%' then a.[net_amt] else '0' end  as online_sales,
case when upper([Transaction_Source]) like '%E-Commerce%' then [net_qty] else '0' end  as online_units
from netsuite.sales_daily_line_history as a
 --where item_name not like '%Open Item (Bin)%'
 left outer join 
 (
 select  [Document_Number], RIGHT('000' + trim(LEFT(division,3)),3) as division,substring(item_name,4,6) as item_name,[net_amt] ,[sku_number],[line_sequence_number]  from netsuite.sales_daily_line_history  where   [division] &lt;&gt; ' ' and  RIGHT('000' + trim(LEFT(division,3)),3) &lt;&gt;70   and  [Document_Number] 
  in --('INV4826','INV4825')
  (
 SELECT distinct [sales_document_number] as sales_document_number  FROM [netsuite].[sales_daily_payment_history] where [payment_method]   like '%Lease%' -- and [eis_number] &lt;&gt; 'ALL' 
 and [sales_document_number] &lt;&gt;' '  )
  ) b 
 on b.[document_number] =a.[Document_Number]   and RIGHT('000' + trim(LEFT(a.division,3)),3)=b.Division  and substring(a.item_name,4,6)=b.[Item_Name] --and a.[line_sequence_number] =b.[line_sequence_number] 
 where  a.[sho_item_type] &lt;&gt;' '    and
a.category &lt;&gt; ' '    and 
 isnumeric(substring(a.item_name,4,6)) =1</property>
